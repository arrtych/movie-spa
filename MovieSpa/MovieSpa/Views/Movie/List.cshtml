@model IEnumerable<MovieSpa.Models.Movie>
<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Document</title>
</head>
<body>
    @{
        ViewBag.Title = "List";
    }

    <h2>List</h2>
    @foreach (var item in Model)
    {
        <h4>ID: @item.Id</h4>
        <h4>Title: @item.Title</h4>
        <h4>Genre: @item.Genre</h4>
        <h4>Year: @item.Year</h4>
        <h4>Rating: @item.Rating</h4>
        
        <hr/>
    }
</body>
</html>

